<div class="todoList">
    <h2>TodoList</h2>

    <form id="placeForm" method="post"  action="/places" > <!--method="post"  action="/places"-->
        <div class="form-group">
            <label for="Place">Place</label>
            <input type="text" class="form-control" id="place" placeholder="Enter Place" name="place">
            <small id="" class="form-text text-muted">Try a place you've never seen.</small>
        </div>
    
        <div class="form-check">
            <label class="form-check-label" for="exampleCheck1">Been There Already?</label>
            <input type="checkbox" class="form-check-input" id="exampleCheck1" name="beenThere" value="TRUE">
        </div>
        <button type="submit" class="btn btn-primary">Submit</button>
    </form>
    
</div>

<div id="userPlaces" class="container-fluid">
    <div class="row">
        <div class="col-sm-6">
            <h2 class="subheader">Todo</h2>
            <div id="todoPlaces"> 
        
            </div>
        </div>
        <div class="col-sm-6">   
            <h2 class="subheader">Complete</h2> 
            <div id="completePlaces">
        
            </div>
        </div>
    </div>
</div>

